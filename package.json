{
  "private": true,
  "name": "full-stack-monorepo",
  "workspaces": [
    "frontend",
    "backend",
    "packages/*"
  ],
  "version": "0.0.1",
  "scripts": {
    "start": "turbo dev",
    "build": "turbo build",
    "orval": "orval --clean && orval --prettier",
    "clean:workspace": " turbo clean && yarn clean:turbo && yarn clean:node-modules",
    "clean:turbo": "rimraf .turbo",
    "clean:node-modules": "rimraf node_modules",
    "lint": "eslint packages frontend backend",
    "lint:fix": "eslint packages frontend backend --fix",
    "typecheck": "turbo typecheck",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/types": "^19.5.0",
    "@stylistic/eslint-plugin": "^4.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "3.0.6",
    "eslint": "^9.20.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "orval": "^7.5.0",
    "prettier": "^3.5.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.6",
    "turbo": "^2.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "5.4.14",
    "vitest": "^3.0.6"
  },
  "proxy": "http://localhost:3000",
  "packageManager": "yarn@4.6.0"
}
